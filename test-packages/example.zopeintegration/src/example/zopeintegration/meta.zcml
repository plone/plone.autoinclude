<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:meta="http://namespaces.zope.org/meta"
    >

  <meta:provides feature="zopeintegration" />
  <!-- Disable z3c.autoinclude -->
  <meta:provides feature="disable-autoinclude" />

  <include
      package="plone.autoinclude"
      file="meta.zcml"
      />
  <autoIncludePlugins
      file="meta.zcml"
      target="zope"
      />

  <!-- FOR TESTS: This is meta.zcml from example.zopeintegration. -->
</configure>
